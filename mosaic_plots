#Download the Topic of Choice repository from Github and move the folder to the H: Drive

data <- read.csv("H:/Topic-of-choice-master/data.csv", sep = ',', header = T)
summary(data)

"BLAST Hits for CREs" <- table(data$ï..Species, data$gut_CRE_BLASTed_against)
`BLAST Hits for CREs`
mosaicplot(`BLAST Hits for CREs`)

"% Identities" <-table(data$ï..Species, data$X.Identity)
`% Identities`
 mosaicplot(`% Identities`)

"% Gaps" <-table(data$ï..Species, data$X.Gap)
`% Gaps`
mosaicplot(`% Gaps`)
